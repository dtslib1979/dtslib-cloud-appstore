{
    "rewrites": [
        { "source": "/youtuber", "destination": "/youtuber/index.html" },
        { "source": "/youtuber/", "destination": "/youtuber/index.html" },
        { "source": "/educator", "destination": "/coming-soon.html" },
        { "source": "/educator/", "destination": "/coming-soon.html" },
        { "source": "/designer", "destination": "/coming-soon.html" },
        { "source": "/designer/", "destination": "/coming-soon.html" },
        { "source": "/marketer", "destination": "/coming-soon.html" },
        { "source": "/marketer/", "destination": "/coming-soon.html" }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                { "key": "X-Content-Type-Options", "value": "nosniff" },
                { "key": "X-Frame-Options", "value": "DENY" },
                { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
            ]
        },
        {
            "source": "/youtuber/auto-shorts/:path*",
            "headers": [
                { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
                { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" }
            ]
        },
        {
            "source": "/youtuber/lecture-shorts/:path*",
            "headers": [
                { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
                { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" }
            ]
        },
        {
            "source": "/youtuber/lecture-long/:path*",
            "headers": [
                { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
                { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" }
            ]
        },
        {
            "source": "/youtuber/clip-shorts/:path*",
            "headers": [
                { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
                { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" }
            ]
        },
        {
            "source": "/youtuber/audio-studio/:path*",
            "headers": [
                { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
                { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" }
            ]
        },
        {
            "source": "/clip-shorts/:path*",
            "headers": [
                { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
                { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" }
            ]
        },
        {
            "source": "/lecture-shorts/:path*",
            "headers": [
                { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
                { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" }
            ]
        },
        {
            "source": "/lecture-long/:path*",
            "headers": [
                { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
                { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" }
            ]
        },
        {
            "source": "/auto-shorts/:path*",
            "headers": [
                { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
                { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" }
            ]
        }
    ]
}
